{
  "compileOnSave": false,
  "compilerOptions": {
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "downlevelIteration": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": false,
    "experimentalDecorators": true,
    "target": "es2015",
    "allowJs": true,
    "skipLibCheck": true,
    "typeRoots": [],
    "lib": [
      "es2018",
      "dom"
    ],
    "module": "esnext",
    "baseUrl": "./"
  }
}
